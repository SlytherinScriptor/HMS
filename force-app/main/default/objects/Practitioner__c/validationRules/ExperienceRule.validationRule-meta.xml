<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ExperienceRule</fullName>
    <active>true</active>
    <errorConditionFormula>TODAY()- Date_of_Birth__c &lt;  (Experience__c *365)</errorConditionFormula>
    <errorMessage>Experience cannot be more than the Age</errorMessage>
</ValidationRule>
